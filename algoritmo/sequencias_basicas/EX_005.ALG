Algoritmo "ex_005"

Var
   nota1, nota2, media : real

Inicio
   //Entrada de dados
   escreva("Digite a primeira nota: ")
   leia(nota1)
   escreva("Digite a segunda nota: ")
   leia(nota2)

   media <- (nota1 + nota2) / 2 //Fórmula para calcular a média

   //Saída de dados
   escreva("A média foi de: ", media)

Fimalgoritmo