Algoritmo "ex_009"

Var
   carteira, dolar : real

Inicio
   //Entrada de dados
   escreva("Digite o valor na carteira: R$")
   leia(carteira)

   dolar <- carteira / 3.45 //Fórmula para converter R$ em US$

   //Saída de dados
   escreva("Você pode comprar no total em dólar: US$", dolar)

Fimalgoritmo