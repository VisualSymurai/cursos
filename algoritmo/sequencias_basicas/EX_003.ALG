Algoritmo "ex_003"

Var
   nome : caractere
   salario : real

Inicio
   //Entrada de dados
   escreva("Digite seu nome: ")
   leia(nome)
   escreva("Digite seu salário: R$")
   leia(salario)

   //Saída de dados
   escreva("O salário de ", nome, " é de R$", salario:3:3)

Fimalgoritmo